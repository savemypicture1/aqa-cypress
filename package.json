{
  "name": "aqa-cypress",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:stable": "cypress run",
    "test:stable:open": "cypress open",
    "test:buggy": "cypress run --config-file cypress.config.buggy.js",
    "test:buggy:open": "cypress open --config-file cypress.config.buggy.js",
    "merge-reports": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
    "generate-report": "npx marge mochawesome.json",
    "report": "npm run merge-reports && npm run generate-report"
  },
